#!/bin/csh
#
# File: $ECMWFHOME/use/use.doublemagics
#
# Purpose: Set the environment for double precision version of MAGICS
#
# Authors: Petra Kogel
#
# $Revision: 1.2 $    $Date: 1997/11/11 09:54:10 $
#
################################################################################

#
# Check if doublemagics is already selected
#
if ($?envSelected == 0) setenv envSelected ""
foreach Keyword ($envSelected)
	if ($Keyword == "doublemagics-5.4") exit
end

#
# Set MAGLIB
#
unsetenv MAG_COASTLINES
setenv MAGLIB "-lmag_r8 -lfgks_r8 -ldgks.2 -lcgks.2"
setenv EMOSLIB "-lemos_r8.new"

#
# Mark environment doublemagics as read
#
setenv envSelected "doublemagics-5.4 $envSelected"
