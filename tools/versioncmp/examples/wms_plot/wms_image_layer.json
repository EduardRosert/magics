{
  "height":"${height=800}",
  "method":"wrep",
  "version":"3.0",
  "width":"${width=800}",
  "drivers":[
    {
      "format":"${format=png}",
      "fullname":"${path=map.png}",
      "output_cairo_transparent_background":"on",
      "output_name_first_page_number":"off",
      "output_width":"${width=800}"
    }
  ]
,
  "page":
  {
    "page_id":"off",
    "meta":
    {
      "javascript_path":"${js=js.js}"
    },
    "map":
    {
      "border":"off",
      "fitted_mode":"${mode=crop}",
      "margin_bottom":"0",
      "margin_left":"0",
      "margin_right":"0",
      "margin_top":"0",
      "plot":
      {
        "grib":
        {
          "input_file_name":"${data=data.grib}"
        },
        "image":
        {
        }
      },
      "nolegend":
      {
        "blanking":"on",
        "border":"off",
        "bottom":"0%",
        "display":"absolute",
        "height":"100%",
        "left":"0%",
        "legend_display_type":"continuous",
        "legend_text_colour":"navy",
        "legend_text_height":"0.4",
        "width":"100%"
      }
    }
  }
}
