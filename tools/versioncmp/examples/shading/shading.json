{
    "drivers": [
        {
            "format": "${format=png}", 
            "fullname": "${path=map.png}", 
            "output_cairo_transparent_background": "on", 
            "output_name_first_page_number": "off", 
            "output_width": "${width=800}"
        }
    ], 
    "height": "${height=800}", 
    "method": "wrep", 
    "page": {
        "map": {
            "border": "off", 
            "fitted_mode": "${mode=crop}", 
            "lower_left_latitude": "-90", 
            "lower_left_longitude": "-180", 
            "margin_bottom": "0", 
            "margin_left": "0", 
            "margin_right": "0", 
            "margin_top": "0", 
            "nolegend": {
                "blanking": "on", 
                "border": "off", 
                "bottom": "0%", 
                "display": "absolute", 
                "height": "100%", 
                "left": "0%", 
                "legend_display_type": "continuous", 
                "legend_text_colour": "navy", 
                "legend_text_height": "0.4", 
                "width": "100%"
            }, 
            "plot": {
                "contour": {
                    "contour": "off", 
                    "contour_highlight_contour": "dash", 
                    "contour_highlight_frequency": 100, 
                    "contour_highlight_thickness": 5, 
                    "contour_hilo": "off", 
                    "contour_interval": 4, 
                    "contour_label": "off", 
                    "contour_label_quality": "high", 
                    "contour_legend_text": "Contour shade (Range: -52 / 48)", 
                    "contour_level_selection_type": "interval", 
                    "contour_line_contour": "dot", 
                    "contour_line_thickness": 3, 
                    "contour_shade": "on", 
                    "contour_shade_colour_list": "rgb(0.88,0.2,0.88)/rgb(0.68,0.2,0.68)/rgb(0.48,0.2,0.48)/rgb(0.28,0.2,0.28)/rgb(0.2,0,0.4)/rgb(0.35,0,0.5)/blue_purple/greenish_blue/rgb(0,0.8,1.0)/blue_green/bluish_green/yellow_green/greenish_yellow/rgb(1,1,0.5)/yellow/orange_yellow/yellowish_orange/rgb(1,0.45,0)/red/rgb(0.8,0,0)/burgundy/rose/magenta/rgb(1,0.5,1)/rgb(1,0.75,1)", 
                    "contour_shade_colour_method": "list", 
                    "contour_shade_max_level": 48, 
                    "contour_shade_method": "area_fill", 
                    "contour_shade_min_level": -52
                }, 
                "grib": {
                    "input_file_name": "${data=data.grib}"
                }
            }, 
            "projection": "cylindrical", 
            "upper_right_latitude": "90", 
            "upper_right_longitude": "180"
        }, 
        "meta": {
            "javascript_path": "${js=js.js}"
        }, 
        "page_id": "off"
    }, 
    "version": "3.0", 
    "width": "${width=800}"
}