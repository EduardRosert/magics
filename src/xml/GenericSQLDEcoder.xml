<?xml-stylesheet type='text/css' href='parameter.css'?>
<magics>
<class name='GenericSQLDecoder' directory='decoders' xmltag='genericsql' prefix='sql'>
	<documentation>
		Here comes the description of the GenericSQLDcoder
	</documentation>
        <parameter name='sql_database' from='string' to='string'
            member='database' default='obs.bufr' >
            <documentation> Generic SQL Database Path </documentation>
        </parameter>
          <parameter name='sql_query' from='string' to='string'
            member='query' default=''>
            <documentation> sql Query </documentation>
        </parameter>
         <parameter name='sql_latitude' from='string' to='string'
            member='latitude' default='latitude'>
            <documentation> sql latitude column name </documentation>
        </parameter>
       <parameter name='sql_longitude' from='string' to='string'
            member='longitude' default='longitude'>
            <documentation> sql longitude column name </documentation>
        </parameter>
        <parameter name='sql_observation' from='string' to='string'
            member='observation' default='observation' >
         
            <documentation> sql observation column name </documentation>
        </parameter>
        <parameter name='sql_level' from='string' to='string'
            member='level' default='press' >
           
            <documentation> sql level column name </documentation>
        </parameter>
        <parameter name='sql_x' from='string' to='string'
            member='x' default='x'>
         
            <documentation> sql  column name used as X input for curve plotting </documentation>
        </parameter>
        <parameter name='sql_y' from='string' to='string'
            member='y' default='y'>
            <documentation> sql  column name used as Y input for curve plotting </documentation>
        </parameter>
	
	
</class>


</magics>
