{
  "version":"3.0",
  "drivers":[
    {
      "format":"png",
      "output_name":"plotting"
    }
  ]
,
  "page":
  {
    "map":
    {
      "fitted_mode":"crop",
      "margin_bottom":"0",
      "margin_left":"0",
      "margin_right":"0",
      "margin_top":"0",
      "coastlines":
      {
        "map_coastline_colour":"grey",
        "map_coastline_land_shade":"on",
        "map_coastline_land_shade_colour":"grey",
        "map_coastline_sea_shade":"on",
        "map_coastline_sea_shade_colour":"white",
        "map_grid":"off",
        "map_label":"off"
      },
      "plot":
      {
        "geopoints":
        {
          "geo_input_file_name":"../airep.geo"
        },
        "symbol":
        {
          "legend":"on",
          "symbol_advanced_table_colour_direction":"clockwise",
          "symbol_advanced_table_interval":"5.000000",
          "symbol_advanced_table_max_level_colour":"red",
          "symbol_advanced_table_min_level_colour":"blue",
          "symbol_advanced_table_selection_type":"interval",
          "symbol_marker":"15",
          "symbol_table_mode":"advanced",
          "symbol_type":"marker"
        }
      },
      "text":
      {
        "bottom":"0.5cm",
        "display":"absolute",
        "height":"1.5cm",
        "left":"0",
        "text_border":"on",
        "text_border_colour":"black",
        "text_box_blanking":"on",
        "text_colour":"black",
        "text_font_size":"0.6",
        "text_justification":"left",
        "text_line_1":"Monitoring of Airep data",
        "width":"8cm"
      },
      "legend":
      {
        "bottom":"10%",
        "height":"80%",
        "left":"90%",
        "legend_blanking":"on",
        "legend_border":"on",
        "legend_display_type":"continuous",
        "legend_text_colour":"black",
        "margin_bottom":"5%",
        "margin_top":"5%",
        "width":"7%"
      }
    }
  }
}