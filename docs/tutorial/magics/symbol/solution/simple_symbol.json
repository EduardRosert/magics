{
  "version":"3.0",
  "drivers":[
    {
      "format":"png",
      "output_name":"simple_plotting"
    }
  ]
,
  "page":
  {
    "map":
    {
      "fitted_mode":"crop",
      "margin_bottom":"0",
      "margin_left":"0",
      "margin_right":"0",
      "margin_top":"0",
      "coastlines":
      {
        "map_coastline_colour":"grey",
        "map_coastline_land_shade":"on",
        "map_coastline_land_shade_colour":"grey",
        "map_coastline_sea_shade":"on",
        "map_coastline_sea_shade_colour":"white",
        "map_grid":"off",
        "map_label":"off"
      },
      "plot":
      {
        "geopoints":
        {
          "geo_input_file_name":"../airep.geo"
        },
        "symbol":
        {
          "symbol_colour":"evergreen",
          "symbol_height":"0.100000",
          "symbol_marker":"15",
          "symbol_type":"marker"
        }
      }
    }
  }
}