<magics version='3'>

    <drivers>
        <ps            name= 'taylor_1' />
        <gif_animation name= 'taylor_1' />
    </drivers>

    <page >
        <nopageid />
        <nologo />

        <map margin_bottom='10%' margin_left='10%' border='off'>  

            <noframe/>    
            <taylor min ='0' max ='1.5' />

            <horizontal_axis>
                <label colour = 'black' height = '0.4'/> 
                <regular  interval='0.1'/>
                <title text = 'Time' colour = 'black' height = '0.5'/>	
                <line  colour = 'black'/>			
            </horizontal_axis>

            <vertical_axis>
                <label colour = 'black' height = '0.4'/> 
                <regular  interval='0.1'/>
                <title text = 'Time' colour = 'black' height = '0.5'/>	
                <line  colour = 'black'/>			
            </vertical_axis>

            <taylorgrid reference='1'  
                        taylor_primary_grid_reference='0.7'  
                        taylor_primary_grid_increment='0.2' 
                        taylor_primary_grid_line_style='dash' 
                        taylor_secondary_grid='on'
                        taylor_secondary_grid_line_colour='evergreen' 
                        taylor_secondary_grid_line_style='dot'
                        taylor_secondary_grid_increment='0.2' /> 

            <plot>
                <mapgen mapgen_input_file_name = 'data/taylor_1.mapgen' />
                <symbol>
                    <advanced symbol_advanced_table_selection_type='interval'
                              symbol_advanced_table_interval='1'
                              symbol_advanced_table_height_list='0.2'
                              symbol_advanced_table_text_display_type='right' 
                              symbol_advanced_table_text_list='obs1/obs2/obs3/obs4/obs5/obs6/obs7/obs8/obs9/obs10' />
                </symbol>
            </plot>
        </map>

        <text  margin_bottom='92%' justification='left' colour = 'black'> <font> 
        Taylor Diagram </font>
        </text>

        <legend  width='10%'  left='70%' bottom='5%' height='80%'
                 legend_text_colour='navy' legend_text_height='0.4'
                 border='off' blanking='off' />   

    </page>

</magics>


