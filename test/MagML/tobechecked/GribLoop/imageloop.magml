<magics version='3.0'>
  <drivers>

     <kml name='meteosat'/>
  </drivers>
<page>
  <gribloop loop_path='meteosat.grb' >
    <loopondate loop_step_span='6' />
      <map>
        <cylindrical >
           <corners lower_left_latitude='-60' upper_right_max_latitude='60' lower_left_longitude='-60' upper_right_longitude='60'/>
        </cylindrical>
            <layer>
              <grib/>
              <image level_count='20' pixel_selection_frequency='30'>
                <lut_equidistant/>
                <lut_computed max_level_colour='black' min_level_colour='white'/>
              </image>   
            </layer>
        </map>
    
    </gribloop>
</page>    
</magics>
