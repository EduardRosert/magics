<magics version='1.0'>
    <page format ='a4' orientation = 'landscape' >
    <subpage>
       <mapview> 
         <cylindrical>
            <corners min_longitude = '-20'
                     min_latitude  = '20'
                     max_longitude = '30'
                     max_latitude  = '60' />
          </cylindrical>
       </mapview>

       <coastlines>
         <coast colour = 'red' />
       </coastlines>

       <layer>
            <grib path = '../data/z500.grb'/>
            <geocontour/>
       </layer>

       <text font = 'Times' colour = 'rgb(0.1,0.6,0.2)'>Z500 Contour Plot</text>

    </subpage>
    </page>
</magics>
