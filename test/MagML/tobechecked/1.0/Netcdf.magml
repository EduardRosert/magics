        
<magics_plot page_x_position = '10'>
  <coastlines map_coastline_colour = 'tan' map_grid_colour = "tan" > </coastlines>
  <plot >
        <netcdf 
			netcdf_filename = 'z500.nc'
			netcdf_field_variable_name = 'z'>
        </netcdf>
        <psymb  
            symbol_table_mode = 'ON'
            symbol_min_table = '40000/45000/50000/55000' 
            symbol_max_table ='45000/50000/55000/60000'
            symbol_height_table ='0.2/0.5/0.2/0.5'
            symbol_marker_table ='0/1/0/1' 
            symbol_colour_table ='blue/green/orange/red' >
        </psymb>
   </plot>
   <title  title_colour = 'magenta' > </title>
   <legend legend_text_colour = 'magenta'>  </legend>
</magics_plot>
   
    
