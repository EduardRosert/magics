<magics>        
    <page>
         <subpage height="90%" bottom='6.%'>
            <axisview height="70%" bottom="10.0%" width="85%" left='10%' >
                 <frame thickness="2" colour='kelly_green'/>	    
				<area x_min="-90" x_max="80" y_min="10000" y_max="20000"/>
				<horizontal_axis colour='kelly-green' style='dash'>
			        <nogrid line_style="dot" colour='kelly_green'/>
				</horizontal_axis>
				<vertical_axis>
                    <nogrid line_style="dot" colour='kelly_green'/>
                </vertical_axis>  
				<xylayer>
                    <netcdf >
                        <matrix path='../data/ECMWF-05a_MF_peaked.nc'
                         netcdf_field_variable_name='mf_vv'
                         netcdf_x_variable_name='lat'
                         netcdf_y_variable_name='plev'/>
                    </netcdf>
                    <contour> 
                        <!--linear/-->
                        <isoline line_colour='grey'> 
                            <count level_count='2'/>
                            <shading>
                                <polygon_shading>
                                    <area_fill/>
                                </polygon_shading> 
                                <!--list 
                                    shade_colour_list='blue/greenish_blue/cyan/green/greenish_yellow/yellow/orange/red_orange/purple/evergreen/black'/-->
                            </shading>
                           <nohighlight/> 
                       
					    </isoline>
					    <hilo/>
                    </contour>    
                </xylayer>
			</axisview>
           <legend>
                <positional x='26.5' y='7' width='2.5' height='5' border='on' blanking='on'>
                </positional>
            </legend>
            <text> Nils experimental plot [mf_vv] </text>
          </subpage>
        </page>
   </magics>
      
