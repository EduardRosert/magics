        
<magics_plot >
  <variable name = 'param' default ='1'>
  <coastlines map_coastline_colour = 'tan' map_grid_colour = "tan" > </coastlines>
   <plot >
        <odb 
            odb_database = '/scratch/graphics/cgs/ECMA.amsub'
            odb_query ='
			SELECT degrees(lat) as latitude, 
			       degrees(lon) as longitude, 
			       obsvalue as colour , 
			       biascorr as height, 
			       fg_depar as marker
			 FROM   hdr, body, sat, atovs, atovs_pred, errstat
			 WHERE  
			        obstype=7 AND 
				sensor=4 AND 
				press=1 and 
				km(lat,lon,radians(0),radians(0)) &lt; 50000'
		    odb_observation = 'obsvalue'				  >

        </odb>
        <psymb  
            symbol_table_mode = 'multi'
            symbol_height_min_value = '-10' 
	    symbol_height_max_value = '10' 
	    symbol_height_count = '20' 
	    symbol_color_selection_type = 'list'
	    symbol_colour_list = '150/165/180/195/210/225/240/255/270/285/300' 
	    symbol_marker_min_value = '-10' 
	    symbol_marker_max_value = '10' 
            symbol_height_table ='0.01/0.02/0.03/0.01/0.02/0.03/0.01/0.02/0.03/0.04/'
            symbol_marker_table ='0/1/15/0/1/15/0/1/15/1' 
            symbol_colour_table ='blue/GREENISH_BLUE/CYAN/GREEN/GREENISH_YELLOW/YELLOW/ORANGE/RED_ORANGE/RED/PURPLE'>
        </psymb>
        </plot>
   <title  title_colour = 'magenta' > </title>
   <legend legend_text_colour = 'magenta'>  </legend>
</magics_plot>
   
    
