  <plot >
        <netcdf 
			netcdf_filename = 'z500.nc'
			netcdf_field_variable_name = 'z' 
			>
        </netcdf>
        <psymb  
            symbol_table_mode = 'multi'
            symbol_height_min_value = '45000' 
	    symbol_height_max_value = '60000' 
	    symbol_height_count = '2' 
	    symbol_color_selection_type = 'list'
