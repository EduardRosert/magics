<magics>        
    <page>
         <subpage height="90%" bottom='6.%'>
            <axisview height="70%" bottom="10.0%" width="85%" left='10%' >
                 <frame thickness="2" colour='kelly_green'/>	    
				<area x_min="0" x_max="393720" y_min="1000" y_max="1">
                    <x_regular x_min="0" x_max="30000"/>
                    <y_logarithmic y_min="1000" y_max="1"/>
                </area>
				<horizontal_axis colour='kelly-green' style='dash'>
                   			        <nogrid line_style="dot" colour='kelly_green'/>
				</horizontal_axis>
				<vertical_axis> <logarithmic/>
                    <nogrid line_style="dot" colour='kelly_green'/>
                </vertical_axis>  
				<xylayer>
                    <netcdf >
                        <matrix path='../data/era40_ml2pl_195709_200208.netcdf'
                         netcdf_field_variable_name='13100000001'
                         netcdf_x_variable_name='time'
                         netcdf_y_variable_name='height'/>
                    </netcdf>
                    <contour> 
                        <!--linear/-->
                      
                        <isoline line_colour='grey'> 
                            <count level_count='10'/>
                            <!--list level_list='-1/-0.75/-0.5/-0.25/0/0.25/0.5/0.75/1'/-->
                            <shading>
                                <polygon_shading>
                                    <area_fill/>
                                </polygon_shading> 
                                <!--list 
                                    shade_colour_list='blue/greenish_blue/cyan/green/greenish_yellow/yellow/orange/red_orange/purple/evergreen/black'/-->
                                <calculate shade_min_level_colour='blue' shade_max_level_colour='red' shade_colour_direction='clockwise'/>
                            </shading>
                           <nohighlight/> 
                       
					    </isoline>
					    <nohilo/>
                    </contour>    
                </xylayer>
			</axisview>
           <legend>
                <positional x='25.7' y='1.9' width='2.5' height='5' border='on' blanking='on'>
                </positional>
            </legend>
            <text> Era40 experimental plot </text>
          </subpage>
        </page>
   </magics>
   
      
