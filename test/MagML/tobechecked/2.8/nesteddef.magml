
<magics version='3.0' format='a4'>                      
    

    <definition>
		

                <coast id='blueline' colour = 'blue'/>
                <coast id='redline' colour = 'red'/>
						
                <coastlines id='blue'>
                    <coast use_id='blueline'/>
                </coastlines>
                <coastlines id='red'>
                    <coast use_id='redline'/>
                </coastlines>
                

		
	</definition>

    <page>                      
       
            <map>                <!--(1 or more allowed)-->
				

		<cylindrical subpage_lower_left_longitude = '-20'
		 	     subpage_lower_left_latitude = '25'
			     subpage_upper_right_longitude = '30'
			     subpage_upper_right_latitude = '65' />
	
             
                 <coastlines  use_id='${coast=red}'/>

            </map>

		

     
    </page>
</magics>


